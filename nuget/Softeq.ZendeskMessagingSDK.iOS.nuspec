<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
    <metadata>
        <id>Softeq.ZendeskMessagingSDK.iOS</id>
        <version>3.8.5.1</version>
        <authors>Softeq Development Corp.</authors>
        <owners>Softeq Development Corp.</owners>
        <requireLicenseAcceptance>false</requireLicenseAcceptance>
        <title>ZendeskMessagingSDK</title>
        <summary>Xamarin.iOS binding library for Zendesk Messaging SDK</summary>
        <description>Zendesk Messaging SDK for Xamarin.iOS. The Messaging SDK is a shared library among the Zendesk product SDKs. It is used to share the Messaging UI components among our product SDKs.</description>
        <releaseNotes>New release</releaseNotes>
        <license type="file">LICENSE.txt</license>
        <icon>nuget-icon.png</icon>
        <projectUrl>https://github.com/Softeq/Zendesk-xamarin-ios</projectUrl>
        <copyright>Copyright 2022 Softeq Development Corp.</copyright>
        <tags>zendesk, xamarin.ios, softeq, ios, messaging</tags>
        <dependencies>
            <!-- Podspec: https://github.com/zendesk/messaging_sdk_ios/blob/3.8.5/ZendeskMessagingSDK.podspec -->
            <group targetFramework="Xamarin.iOS1.0">
                <dependency id="Softeq.ZendeskCommonUISDK.iOS" version="6.1.4.1" />
                <dependency id="Softeq.ZendeskMessagingAPISDK.iOS" version="3.8.5.1" />
            </group>
        </dependencies>
    </metadata>
    <files>
        <file src="../LICENSE.txt" target="" />
        <file src="../assets/nuget-icon.png" target="" />

        <file src="../MessagingSDK.Bindings/bin/Release/MessagingSDK.Bindings.dll" target="lib/xamarinios10" />
    </files>
</package>
<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
    <metadata>
        <id>Softeq.ZendeskChatSDK.iOS</id>
        <version>2.12.0.1</version>
        <authors>Softeq Development Corp.</authors>
        <owners>Softeq Development Corp.</owners>
        <requireLicenseAcceptance>false</requireLicenseAcceptance>
        <title>ZendeskChatSDK</title>
        <summary>Xamarin.iOS binding library for Zendesk Chat SDK</summary>
        <description>Zendesk Chat SDK is a quick, convenient way to get live chat into your mobile apps. With just a few lines of code, you can provide your end users with an easy way to get in touch from any mobile app.</description>
        <releaseNotes>New release</releaseNotes>
        <license type="file">LICENSE.txt</license>
        <icon>nuget-icon.png</icon>
        <projectUrl>https://github.com/Softeq/Zendesk-xamarin-ios</projectUrl>
        <copyright>Copyright 2022 Softeq Development Corp.</copyright>
        <tags>zendesk, xamarin.ios, softeq, ios, support</tags>
        <dependencies>
            <!-- Podspec: https://github.com/zendesk/chat_sdk_ios/blob/2.12.0/ZendeskChatSDK.podspec -->
            <group targetFramework="Xamarin.iOS1.0">
                <dependency id="Softeq.ZendeskChatProvidersSDK.iOS" version="2.12.0.1" />
                <dependency id="Softeq.ZendeskMessagingSDK.iOS" version="3.8.5.1" />
            </group>
        </dependencies>
    </metadata>
    <files>
        <file src="../LICENSE.txt" target="" />
        <file src="../assets/nuget-icon.png" target="" />

        <file src="../ChatSDK.Bindings/bin/Release/ChatSDK.Bindings.dll" target="lib/xamarinios10" />
    </files>
</package>